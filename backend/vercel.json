{
  "version": 2,
  "builds": [
      {
          "src": "/src/app.js",
          "use": "@vercel/python"
      }
  ],
  
  "rewrites": [
      { "source": "/(.*)", "destination": "/src/app.js" }
  ],
  "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      },
      {
        "source": "https://client-kappa-black.vercel.app/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }  
  ]
}